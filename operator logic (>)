start

? Переменные
new a number
new b number

? Значения
set a 10
set b 9

? Проверка условия с нашим главным героем (>) и вывод исходя из проверки
if (a > b)
  log string "a > b - true"
else
  log string "a > b - false"
close

? Тут тупо инверсируем? ну тип того в общем обратная запись a < b
if (b > a)
    log string "a < b - true"
else
    log string "a < b - false"
close

? Проверка на равнение, допустим, я хочу к 9
if (b > 8)
  if (10 > b)
    log string "b = 9 - true"
  else
    log string "b = 9 - false"
  close
else
  log string "b = 9 - false"
close

? Проверка на не равнение (!=), допустим, я хочу к 9
if (b > 8)
  if (10 > b)
    log string "b != 9 - false"
  else
    log string "b != 9 - true"
  close
else
  log string "b != 9 - true"
close

? Проверка на больше или равно (>=)
new i number
set i 10
set i i - 1
if (a > i)
	log string "a >= i - true"
else
  	log string "a >= i - false"
close

? Проверка на больше или равно (<=)
new i1 number
set i1 10
set i1 i1 - 1
if (a > i1)
	log string "a <= i1 - false"
else
  	log string "a <= i1 - true"
close
finish
